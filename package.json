  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/babel__register": "^7.17.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/rimraf": "^3.0.2",
    "babel-loader": "^8.2.5",
    "babel-preset-react-app": "^10.0.1",
    "compression": "^1.7.4",
    "concurrently": "^7.4.0",
    "css-loader": "^6.7.1",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.3",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.4.2",
    "resolve": "^1.22.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@tsconfig/create-react-app": "^1.0.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "start": "concurrently \"npm run server:dev\" \"npm run bundler:server:dev\" \"npm run bundler:client:dev\"",
    "server:dev": "NODE_ENV=development nodemon --watch build/server build/server/server.js",
    "bundler:server:dev": "cross-env NODE_ENV=development nodemon --config nodemon.build.json -- scripts/build.server.js",
    "bundler:client:dev": "cross-env NODE_ENV=development nodemon --config nodemon.build.json -- scripts/build.client.js"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ]
  }
}
